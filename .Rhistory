n <- 1000
k <- n/2
u1 <- runif(k, min = 0, max = 1)
u2 <- runif(k, min = 0, max = 1)
z1 <- sqrt(-2*log(u1))*cos(2*pi*u2)
z2 <- sqrt(-2*log(u1))*sin(2*pi*u2)
z <- c(z1, z2)
mu   <- 5
sigma <- 2
x <- mu + sigma * z
mean(x)
sd(x)
hist(x,freq = FALSE,ylab = "Density", xlab = "$x$")
curve(dnorm(x, mean = mu , sd = sigma ), col = "red" , lwd = 2 , add = TRUE)
